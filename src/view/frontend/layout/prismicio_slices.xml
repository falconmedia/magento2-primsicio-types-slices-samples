<?xml version="1.0" ?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="prismicio_slices_action_blocks" />
    <update handle="prismicio_slices_amasty_custom_form" />
    <update handle="prismicio_slices_amasty_custom_form_with_image" />
    <update handle="prismicio_slices_banner_with_caption" />
    <update handle="prismicio_slices_categories" />
    <update handle="prismicio_slices_category_content" />
    <update handle="prismicio_slices_contact_block" />
    <update handle="prismicio_slices_divider" />
    <update handle="prismicio_slices_faq_accordion" />
    <update handle="prismicio_slices_faq_items" />
    <update handle="prismicio_slices_faq_section" />
    <update handle="prismicio_slices_full_width_image" />
    <update handle="prismicio_slices_general_card" />
    <update handle="prismicio_slices_hero" />
    <update handle="prismicio_slices_image" />
    <update handle="prismicio_slices_menu_items" />
    <update handle="prismicio_slices_open_closing_times" />
    <update handle="prismicio_slices_product_collection" />
    <update handle="prismicio_slices_products" />
    <update handle="prismicio_slices_quick_card" />
    <update handle="prismicio_slices_seo_text" />
    <update handle="prismicio_slices_slider" />
    <update handle="prismicio_slices_stores" />
    <update handle="prismicio_slices_subcategories_list" />
    <update handle="prismicio_slices_teaser_images" />
    <update handle="prismicio_slices_usps" />

    <body>
        <block name="prismicio.slices" class="Elgentos\PrismicIO\Block\Template" template="Elgentos_PrismicIO::modules/prismic.slices.phtml">
            <arguments>
                <argument name="reference" xsi:type="string">data</argument>
            </arguments>

            <block name="prismicio.slices.content" class="Elgentos\PrismicIO\Block\Slices" template="body">
                <action method="append">
                    <argument name="element" xsi:type="string">action_blocks</argument>
                    <argument name="alias" xsi:type="string">prismicio_slices_action_blocks</argument>
                </action>

                <action method="append">
                    <argument name="element" xsi:type="string">amasty_custom_form</argument>
                    <argument name="alias" xsi:type="string">prismicio_slices_amasty_custom_form</argument>
                </action>

                <action method="append">
                    <argument name="element" xsi:type="string">amasty_custom_form_with_image</argument>
                    <argument name="alias" xsi:type="string">prismicio_slices_amasty_custom_form_with_image</argument>
                </action>

                <action method="append">
                    <argument name="element" xsi:type="string">banner_with_caption</argument>
                    <argument name="alias" xsi:type="string">prismicio_slices_banner_with_caption</argument>
                </action>

                <action method="append">
                    <argument name="element" xsi:type="string">categories</argument>
                    <argument name="alias" xsi:type="string">prismicio_slices_categories</argument>
                </action>

                <action method="append">
                    <argument name="element" xsi:type="string">category_content</argument>
                    <argument name="alias" xsi:type="string">prismicio_slices_category_content</argument>
                </action>

                    <action method="append">
                        <argument name="element" xsi:type="string">contact_block</argument>
                <argument name="alias" xsi:type="string">prismicio_slices_contact_block</argument>
                </action>

                <action method="append">
                    <argument name="element" xsi:type="string">divider</argument>
                    <argument name="alias" xsi:type="string">prismicio_slices_divider</argument>
                </action>

                <action method="append">
                    <argument name="element" xsi:type="string">faq_accordion</argument>
                    <argument name="alias" xsi:type="string">prismicio_slices_faq_accordion</argument>
                </action>

                <action method="append">
                    <argument name="element" xsi:type="string">faq_items</argument>
                    <argument name="alias" xsi:type="string">prismicio_slices_faq_items</argument>
                </action>

                <action method="append">
                    <argument name="element" xsi:type="string">faq_section</argument>
                    <argument name="alias" xsi:type="string">prismicio_slices_faq_section</argument>
                </action>

                <action method="append">
                    <argument name="element" xsi:type="string">full_width_image</argument>
                    <argument name="alias" xsi:type="string">prismicio_slices_full_width_image</argument>
                </action>

                <action method="append">
                    <argument name="element" xsi:type="string">general_card</argument>
                    <argument name="alias" xsi:type="string">prismicio_slices_general_card</argument>
                </action>

                <action method="append">
                    <argument name="element" xsi:type="string">hero</argument>
                    <argument name="alias" xsi:type="string">prismicio_slices_hero</argument>
                </action>

                <action method="append">
                    <argument name="element" xsi:type="string">image</argument>
                    <argument name="alias" xsi:type="string">prismicio_slices_image</argument>
                </action>

                <action method="append">
                    <argument name="element" xsi:type="string">menu_items</argument>
                    <argument name="alias" xsi:type="string">prismicio_slices_menu_items</argument>
                </action>

                <action method="append">
                    <argument name="element" xsi:type="string">open_closing_times</argument>
                    <argument name="alias" xsi:type="string">prismicio_slices_open_closing_times</argument>
                </action>

                <action method="append">
                    <argument name="element" xsi:type="string">product_collection</argument>
                    <argument name="alias" xsi:type="string">prismicio_slices_product_collection</argument>
                </action>

                <action method="append">
                    <argument name="element" xsi:type="string">products</argument>
                    <argument name="alias" xsi:type="string">prismicio_slices_products</argument>
                </action>

                <action method="append">
                    <argument name="element" xsi:type="string">quick_card</argument>
                    <argument name="alias" xsi:type="string">prismicio_slices_quick_card</argument>
                </action>

                <action method="append">
                    <argument name="element" xsi:type="string">seo_text</argument>
                    <argument name="alias" xsi:type="string">prismicio_slices_seo_text</argument>
                </action>

                <action method="append">
                    <argument name="element" xsi:type="string">slider</argument>
                    <argument name="alias" xsi:type="string">prismicio_slices_slider</argument>
                </action>

                <action method="append">
                    <argument name="element" xsi:type="string">stores</argument>
                    <argument name="alias" xsi:type="string">prismicio_slices_stores</argument>
                </action>

                <action method="append">
                    <argument name="element" xsi:type="string">subcategories_list</argument>
                    <argument name="alias" xsi:type="string">prismicio_slices_subcategories_list</argument>
                </action>

                <action method="append">
                    <argument name="element" xsi:type="string">teaser_images</argument>
                    <argument name="alias" xsi:type="string">prismicio_slices_teaser_images</argument>
                </action>

                <action method="append">
                    <argument name="element" xsi:type="string">usps</argument>
                    <argument name="alias" xsi:type="string">prismicio_slices_usps</argument>
                </action>

            </block>
        </block>
    </body>
</page>
